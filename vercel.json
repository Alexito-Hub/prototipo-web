{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "includeFiles": [
        "src/views/**/*",
        "src/public/**/*"
    ],
    "functions": {
      "server.js": {
        "memory": 3008,
        "maxDuration": 10
      }
    }
  }
  